innodb_autoextend_increment = 1000
innodb_strict_mode=0
sql_mode=NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION
performance_schema=ON
connect_timeout=10
innodb_rollback_on_timeout=1
wait_timeout=18000
character-set-server  = utf8mb4 
